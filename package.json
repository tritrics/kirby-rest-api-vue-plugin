{
  "name": "kirby-rest-api-vue-plugin",
  "version": "0.9.6",
  "description": "A vue plugin, that is build to communicate easily with tritrics/kirby-rest-api",
  "author": {
    "name": "Michael Adams",
    "email": "ma@tritrics.dk"
  },
  "keywords": [
    "vue plugin",
    "Kirby",
    "rest api"
  ],
  "homepage": "https://github.com/tritrics/kirby-rest-api-vue-plugin",
  "bugs": "https://github.com/tritrics/kirby-rest-api-vue-plugin/issues",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
